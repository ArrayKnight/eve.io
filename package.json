{
    "name": "eve.io",
    "private": true,
    "version": "1.0.0",
    "description": "",
    "license": "MIT",
    "scripts": {
        "clean": "npm-run-all clean:*",
        "clean:deps": "rm -rf node_modules && rm -f package-lock.json && rm -f yarn.lock",
        "clean:dirs": "rm -rf .cache && rm -rf .dev && rm -rf .dist",
        "preinstall": "npm run clean:deps",
        "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "prestart": "npm run clean:dirs",
        "start": "parcel src/index.html --out-dir .dev --open --port 3333",
        "prebuild": "npm run clean:dirs",
        "build": "parcel build src/index.html --out-dir .dist --no-source-maps",
        "upgrade": "npm-check --update --save-exact"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{ts,tsx,json,md}": [
            "prettier --write"
        ],
        "**/*.{ts,tsx}": [
            "eslint"
        ]
    },
    "dependencies": {
        "@emotion/core": "10.0.28",
        "@emotion/styled": "10.0.27",
        "@googlemaps/js-api-loader": "1.0.0",
        "@material-ui/core": "4.9.11",
        "@material-ui/lab": "4.0.0-alpha.50",
        "@types/googlemaps": "3.39.3",
        "axios": "0.19.2",
        "geonames.js": "2.3.0",
        "google-maps-react": "2.0.2",
        "lodash": "4.17.19",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-helmet": "6.0.0",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-transform-runtime": "7.9.0",
        "@babel/preset-env": "7.9.5",
        "@babel/preset-react": "7.9.4",
        "@types/lodash": "4.14.150",
        "@types/node": "13.13.0",
        "@types/react": "16.9.34",
        "@types/react-router": "5.1.5",
        "@types/react-router-dom": "5.1.4",
        "@typescript-eslint/eslint-plugin": "2.28.0",
        "@typescript-eslint/parser": "2.28.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.10.1",
        "eslint-plugin-import": "2.20.2",
        "eslint-plugin-prettier": "3.1.3",
        "eslint-plugin-react": "7.19.0",
        "eslint-plugin-react-hooks": "3.0.0",
        "husky": "4.2.5",
        "lint-staged": "10.1.5",
        "npm-check": "5.9.2",
        "npm-run-all": "4.1.5",
        "parcel-bundler": "1.12.4",
        "prettier": "2.0.4",
        "ts-loader": "7.0.0",
        "ts-node": "8.8.2",
        "tsconfig-paths": "3.9.0",
        "typescript": "3.8.3",
        "wait-on": "4.0.2"
    }
}
